URxvt.font: xft:Hack:pixelsize=12
! disable bold fonts by just applying the same font string:
! URxvt.font: xft:terminus12
! URxvt.boldFont: xft:terminus12
URxvt.foreground: #43a047
URxvt.background: #121212
URxvt.saveLines: 8192
URxvt.scrollBar: false
! do not scroll with output
URxvt.scrollTtyOutput: false
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt.scrollWithBuffer: true
! scroll back to the bottom on keypress
URxvt.scrollTtyKeypress: true
! tabs
!URxvt.perl-ext-common: tabbed
!URxvt.tabbed.tabbar-fg: 2
!URxvt.tabbed.tabbar-bg: 16
!URxvt.tabbed.tab-fg: 3
!URxvt.tabbed.tab-bg: 16

! COLORS SETTINGS
! black
URxvt*color0  : #000000
URxvt*color8  : #555753
! red
URxvt*color1  : #cc0000
URxvt*color9  : #ef2929
! green
URxvt*color2  : #4e9a06
URxvt*color10 : #8ae234
! yellow
URxvt*color3  : #c4a000
URxvt*color11 : #fce94f
! blue
URxvt*color4  : #3465a4
URxvt*color12 : #729fcf
! magenta
URxvt*color5  : #75507b
URxvt*color13 : #ad7fa8
! cyan
URxvt*color6  : #06989a
URxvt*color14 : #34e2e2
! white
URxvt*color7: #dedede
URxvt*color15 : #eeeeec

! colors for rxvt tabs plugin
URxvt*color16: #121212
